{
    "name": "tourhunter-test",
    "description": "Test project for TourHunter company",
    "keywords": ["yii2"],
    "homepage": "https://tourhunter-test.herokuapp.com/",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/hlushko/tourhunter-test/issues",
        "source": "https://github.com/hlushko/tourhunter-test"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.3.0",
        "yiisoft/yii2": "~2.0.14",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0",

        "codeception/base": "^2.2.3",
        "codeception/verify": "~0.3.1",
        "codeception/specify": "~0.4.3",

        "fzaninotto/faker": "^1.8.0",

        "heroku/heroku-buildpack-php": "^148"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "scripts": {
        "post-install-cmd": [
            "yii\\composer\\Installer::postInstall"
        ],
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject",
            "yii\\composer\\Installer::postInstall"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "web/assets": "0777",
                    "yii": "0755"
                }
            ]
        },
        "yii\\composer\\Installer::postInstall": {
            "generateCookieValidationKey": [
                "config/web.php"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "app\\": "",
            "tests\\": "tests"
        }
    }
}
